			reg = <0x12580000 0x100>;
			interrupts = <0x00 0x46 0x04>;
			clocks = <0x07 0x130>;
			clock-names = "usbhost";
			status = "okay";
			phys = <0x25 0x02>;
			phy-names = "hsic0";
