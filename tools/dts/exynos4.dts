/dts-v1/;

/ {
	interrupt-parent = <0x01>;
	#address-cells = <0x01>;
	#size-cells = <0x01>;
	compatible = "hardkernel,odroid-x", "samsung,exynos4412", "samsung,exynos4";
	model = "Hardkernel ODROID-X board based on Exynos4412";

	aliases {
		spi0 = "/soc/spi@13920000";
		spi1 = "/soc/spi@13930000";
		spi2 = "/soc/spi@13940000";
		i2c0 = "/soc/i2c@13860000";
		i2c1 = "/soc/i2c@13870000";
		i2c2 = "/soc/i2c@13880000";
		i2c3 = "/soc/i2c@13890000";
		i2c4 = "/soc/i2c@138a0000";
		i2c5 = "/soc/i2c@138b0000";
		i2c6 = "/soc/i2c@138c0000";
		i2c7 = "/soc/i2c@138d0000";
		i2c8 = "/soc/i2c@138e0000";
		csis0 = "/soc/camera@11800000/csis@80000";
		csis1 = "/soc/camera@11800000/csis@90000";
		fimc0 = "/soc/camera@11800000/fimc@0";
		fimc1 = "/soc/camera@11800000/fimc@10000";
		fimc2 = "/soc/camera@11800000/fimc@20000";
		fimc3 = "/soc/camera@11800000/fimc@30000";
		serial0 = "/soc/serial@13800000";
		serial1 = "/soc/serial@13810000";
		serial2 = "/soc/serial@13820000";
		serial3 = "/soc/serial@13830000";
		pinctrl0 = "/soc/pinctrl@11400000";
		pinctrl1 = "/soc/pinctrl@11000000";
		pinctrl2 = "/soc/pinctrl@3860000";
		pinctrl3 = "/soc/pinctrl@106e0000";
		fimc-lite0 = "/soc/camera@11800000/fimc-lite@b90000";
		fimc-lite1 = "/soc/camera@11800000/fimc-lite@ba0000";
		mmc0 = "/soc/mmc@12550000";
		mmc2 = "/soc/mmc@12530000";
		ethernet = "/soc/usb@12580000/hub@2/hub@1/ethernet@1";
	};

	pmu {
		compatible = "arm,cortex-a9-pmu";
		interrupt-parent = <0x02>;
		status = "okay";
		interrupts = <0x02 0x02 0x03 0x02 0x12 0x02 0x13 0x02>;
		interrupt-affinity = <0x03 0x04 0x05 0x06>;
	};

	soc {
		compatible = "simple-bus";
		#address-cells = <0x01>;
		#size-cells = <0x01>;
		ranges;

		clock-controller@3810000 {
			compatible = "samsung,exynos4210-audss-clock";
			reg = <0x3810000 0x0c>;
			#clock-cells = <0x01>;
			clocks = <0x07 0x03 0x07 0x06 0x07 0x90 0x07 0x90>;
			clock-names = "pll_ref", "pll_in", "sclk_audio", "sclk_pcm_in";
			assigned-clocks = <0x08 0x00 0x08 0x01 0x08 0x02 0x08 0x03 0x08 0x04>;
			assigned-clock-parents = <0x07 0x06 0x08 0x00>;
			assigned-clock-rates = <0x00 0x00 0xbb80001 0x5dc0000 0x1770000>;
			phandle = <0x08>;
		};

		i2s@3830000 {
			compatible = "samsung,s5pv210-i2s";
			reg = <0x3830000 0x100>;
			clocks = <0x08 0x06 0x08 0x03 0x08 0x07>;
			clock-names = "iis", "i2s_opclk0", "i2s_opclk1";
			#clock-cells = <0x01>;
			clock-output-names = "i2s_cdclk0";
			dmas = <0x09 0x0c 0x09 0x0b 0x09 0x0a>;
			dma-names = "tx", "rx", "tx-sec";
			samsung,idma-addr = <0x3000000>;
			#sound-dai-cells = <0x01>;
			status = "okay";
			pinctrl-0 = <0x0a>;
			pinctrl-names = "default";
			assigned-clocks = <0x0b 0x01>;
			assigned-clock-parents = <0x08 0x07>;
			phandle = <0x0b>;
		};

		chipid@10000000 {
			compatible = "samsung,exynos4210-chipid";
			reg = <0x10000000 0x100>;
		};

		snoop-control-unit@10500000 {
			compatible = "arm,cortex-a9-scu";
			reg = <0x10500000 0x2000>;
		};

		memory-controller@12570000 {
			compatible = "samsung,exynos4210-srom";
			reg = <0x12570000 0x14>;
		};

		power-domain@10023c40 {
			compatible = "samsung,exynos4210-pd";
			reg = <0x10023c40 0x20>;
			#power-domain-cells = <0x00>;
			label = "MFC";
			phandle = <0x2c>;
		};

		power-domain@10023c60 {
			compatible = "samsung,exynos4210-pd";
			reg = <0x10023c60 0x20>;
			#power-domain-cells = <0x00>;
			label = "G3D";
			phandle = <0x28>;
		};

		power-domain@10023c80 {
			compatible = "samsung,exynos4210-pd";
			reg = <0x10023c80 0x20>;
			#power-domain-cells = <0x00>;
			label = "LCD0";
			phandle = <0x0c>;
		};

		power-domain@10023c20 {
			compatible = "samsung,exynos4210-pd";
			reg = <0x10023c20 0x20>;
			#power-domain-cells = <0x00>;
			power-domains = <0x0c>;
			label = "TV";
			phandle = <0x44>;
		};

		power-domain@10023c00 {
			compatible = "samsung,exynos4210-pd";
			reg = <0x10023c00 0x20>;
			#power-domain-cells = <0x00>;
			label = "CAM";
			phandle = <0x0f>;
		};

		power-domain@10023ce0 {
			compatible = "samsung,exynos4210-pd";
			reg = <0x10023ce0 0x20>;
			#power-domain-cells = <0x00>;
			label = "GPS";
		};

		power-domain@10023d00 {
			compatible = "samsung,exynos4210-pd";
			reg = <0x10023d00 0x20>;
			#power-domain-cells = <0x00>;
			label = "GPS alive";
		};

		interrupt-controller@10490000 {
			compatible = "arm,cortex-a9-gic";
			#interrupt-cells = <0x03>;
			interrupt-controller;
			reg = <0x10490000 0x10000 0x10480000 0x10000>;
			cpu-offset = <0x4000>;
			phandle = <0x01>;
		};

		interrupt-controller@10440000 {
			compatible = "samsung,exynos4210-combiner";
			#interrupt-cells = <0x02>;
			interrupt-controller;
			reg = <0x10440000 0x1000>;
			interrupts = <0x00 0x00 0x04 0x00 0x01 0x04 0x00 0x02 0x04 0x00 0x03 0x04 0x00 0x04 0x04 0x00 0x05 0x04 0x00 0x06 0x04 0x00 0x07 0x04 0x00 0x08 0x04 0x00 0x09 0x04 0x00 0x0a 0x04 0x00 0x0b 0x04 0x00 0x0c 0x04 0x00 0x0d 0x04 0x00 0x0e 0x04 0x00 0x0f 0x04 0x00 0x6b 0x04 0x00 0x6c 0x04 0x00 0x30 0x04 0x00 0x2a 0x04>;
			samsung,combiner-nr = <0x14>;
			phandle = <0x02>;
		};

		syscon@10010000 {
			compatible = "samsung,exynos4-sysreg", "syscon";
			reg = <0x10010000 0x400>;
			phandle = <0x10>;
		};

		system-controller@10020000 {
			compatible = "samsung,exynos4412-pmu", "simple-mfd", "syscon";
			reg = <0x10020000 0x4000>;
			interrupt-controller;
			#interrupt-cells = <0x03>;
			interrupt-parent = <0x01>;
			clock-names = "clkout0", "clkout1", "clkout2", "clkout3", "clkout4", "clkout8", "clkout9";
			clocks = <0x07 0x17 0x07 0x18 0x07 0x19 0x07 0x1a 0x07 0x1b 0x07 0x01 0x07 0x02>;
			#clock-cells = <0x01>;
			phandle = <0x0d>;

			mipi-phy {
				compatible = "samsung,s5pv210-mipi-video-phy";
				#phy-cells = <0x01>;
				phandle = <0x0e>;
			};

			syscon-poweroff {
				compatible = "syscon-poweroff";
				regmap = <0x0d>;
				offset = <0x330c>;
				mask = <0x5200>;
			};

			syscon-reboot {
				compatible = "syscon-reboot";
				regmap = <0x0d>;
				offset = <0x400>;
				mask = <0x01>;
			};
		};

		dsi@11c80000 {
			compatible = "samsung,exynos4210-mipi-dsi";
			reg = <0x11c80000 0x10000>;
			interrupts = <0x00 0x4f 0x04>;
			power-domains = <0x0c>;
			phys = <0x0e 0x01>;
			phy-names = "dsim";
			clocks = <0x07 0x11e 0x07 0x8f>;
			clock-names = "bus_clk", "sclk_mipi";
			status = "disabled";
			#address-cells = <0x01>;
			#size-cells = <0x00>;
		};

		camera@11800000 {
			compatible = "samsung,fimc";
			ranges = <0x00 0x11800000 0xba1000>;
			status = "okay";
			#address-cells = <0x01>;
			#size-cells = <0x01>;
			#clock-cells = <0x01>;
			clock-output-names = "cam_a_clkout", "cam_b_clkout";
			clocks = <0x07 0x84 0x07 0x85 0x07 0x15f 0x07 0x160>;
			clock-names = "sclk_cam0", "sclk_cam1", "pxl_async0", "pxl_async1";

			fimc@0 {
				compatible = "samsung,exynos4212-fimc";
				reg = <0x00 0x1000>;
				interrupts = <0x00 0x54 0x04>;
				clocks = <0x07 0x100 0x07 0x80>;
				clock-names = "fimc", "sclk_fimc";
				power-domains = <0x0f>;
				samsung,sysreg = <0x10>;
				iommus = <0x11>;
				status = "okay";
				samsung,pix-limits = <0x1080 0x2000 0x780 0x1080>;
				samsung,mainscaler-ext;
				samsung,isp-wb;
				samsung,cam-if;
				assigned-clocks = <0x07 0x180 0x07 0x80>;
				assigned-clock-parents = <0x07 0x11>;
				assigned-clock-rates = <0x00 0xa7d8c00>;
			};

			fimc@10000 {
				compatible = "samsung,exynos4212-fimc";
				reg = <0x10000 0x1000>;
				interrupts = <0x00 0x55 0x04>;
				clocks = <0x07 0x101 0x07 0x81>;
				clock-names = "fimc", "sclk_fimc";
				power-domains = <0x0f>;
				samsung,sysreg = <0x10>;
				iommus = <0x12>;
				status = "okay";
				samsung,pix-limits = <0x1080 0x2000 0x780 0x1080>;
				samsung,mainscaler-ext;
				samsung,isp-wb;
				samsung,cam-if;
				assigned-clocks = <0x07 0x181 0x07 0x81>;
				assigned-clock-parents = <0x07 0x11>;
				assigned-clock-rates = <0x00 0xa7d8c00>;
			};

			fimc@20000 {
				compatible = "samsung,exynos4212-fimc";
				reg = <0x20000 0x1000>;
				interrupts = <0x00 0x56 0x04>;
				clocks = <0x07 0x102 0x07 0x82>;
				clock-names = "fimc", "sclk_fimc";
				power-domains = <0x0f>;
				samsung,sysreg = <0x10>;
				iommus = <0x13>;
				status = "okay";
				samsung,pix-limits = <0x1080 0x2000 0x780 0x1080>;
				samsung,mainscaler-ext;
				samsung,isp-wb;
				samsung,lcd-wb;
				samsung,cam-if;
				assigned-clocks = <0x07 0x182 0x07 0x82>;
				assigned-clock-parents = <0x07 0x11>;
				assigned-clock-rates = <0x00 0xa7d8c00>;
			};

			fimc@30000 {
				compatible = "samsung,exynos4212-fimc";
				reg = <0x30000 0x1000>;
				interrupts = <0x00 0x57 0x04>;
				clocks = <0x07 0x103 0x07 0x83>;
				clock-names = "fimc", "sclk_fimc";
				power-domains = <0x0f>;
				samsung,sysreg = <0x10>;
				iommus = <0x14>;
				status = "okay";
				samsung,pix-limits = <0x780 0x2000 0x556 0x780>;
				samsung,rotators = <0x00>;
				samsung,mainscaler-ext;
				samsung,isp-wb;
				samsung,lcd-wb;
				assigned-clocks = <0x07 0x183 0x07 0x83>;
				assigned-clock-parents = <0x07 0x11>;
				assigned-clock-rates = <0x00 0xa7d8c00>;
			};

			csis@80000 {
				compatible = "samsung,exynos4210-csis";
				reg = <0x80000 0x4000>;
				interrupts = <0x00 0x4e 0x04>;
				clocks = <0x07 0x104 0x07 0x86>;
				clock-names = "csis", "sclk_csis";
				bus-width = <0x04>;
				power-domains = <0x0f>;
				phys = <0x0e 0x00>;
				phy-names = "csis";
				status = "disabled";
				#address-cells = <0x01>;
				#size-cells = <0x00>;
			};

			csis@90000 {
				compatible = "samsung,exynos4210-csis";
				reg = <0x90000 0x4000>;
				interrupts = <0x00 0x50 0x04>;
				clocks = <0x07 0x105 0x07 0x87>;
				clock-names = "csis", "sclk_csis";
				bus-width = <0x02>;
				power-domains = <0x0f>;
				phys = <0x0e 0x02>;
				phy-names = "csis";
				status = "disabled";
				#address-cells = <0x01>;
				#size-cells = <0x00>;
			};

			fimc-lite@b90000 {
				compatible = "samsung,exynos4212-fimc-lite";
				reg = <0xb90000 0x1000>;
				interrupts = <0x00 0x69 0x04>;
				power-domains = <0x15>;
				clocks = <0x16 0x04>;
				clock-names = "flite";
				iommus = <0x17>;
				status = "disabled";
			};

			fimc-lite@ba0000 {
				compatible = "samsung,exynos4212-fimc-lite";
				reg = <0xba0000 0x1000>;
				interrupts = <0x00 0x6a 0x04>;
				power-domains = <0x15>;
				clocks = <0x16 0x05>;
				clock-names = "flite";
				iommus = <0x18>;
				status = "disabled";
			};

			fimc-is@800000 {
				compatible = "samsung,exynos4212-fimc-is";
				reg = <0x800000 0x260000>;
				interrupts = <0x00 0x5a 0x04 0x00 0x5f 0x04>;
				power-domains = <0x15>;
				clocks = <0x16 0x04 0x16 0x05 0x16 0x0e 0x16 0x0d 0x16 0x01 0x16 0x02 0x16 0x03 0x16 0x06 0x16 0x07 0x16 0x0f 0x16 0x14 0x16 0x1b 0x16 0x1c 0x16 0x1d 0x16 0x1e 0x07 0x11 0x07 0x0d 0x07 0x18b 0x07 0x1c6 0x07 0x1c7 0x07 0x17e>;
