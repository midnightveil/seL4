		rtc@10070000 {
			compatible = "samsung,s3c6410-rtc";
			reg = <0x10070000 0x100>;
			interrupt-parent = <0x0d>;
			interrupts = <0x00 0x2c 0x04 0x00 0x2d 0x04>;
			clocks = <0x07 0x15a 0x1d 0x00>;
			clock-names = "rtc", "rtc_src";
			status = "okay";
		};

		keypad@100a0000 {
