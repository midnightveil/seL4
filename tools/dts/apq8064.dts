/dts-v1/;

/ {
	#address-cells = <0x01>;
	#size-cells = <0x01>;
	model = "Qualcomm APQ8064/IFC6410";
	compatible = "qcom,apq8064-ifc6410", "qcom,apq8064";
	interrupt-parent = <0x01>;

	reserved-memory {
		#address-cells = <0x01>;
		#size-cells = <0x01>;
		ranges;

		smem@80000000 {
			reg = <0x80000000 0x200000>;
			no-map;
			phandle = <0x0e>;
		};

		wcnss@8f000000 {
			reg = <0x8f000000 0x700000>;
			no-map;
			phandle = <0x5d>;
		};
	};

	cpus {
		#address-cells = <0x01>;
		#size-cells = <0x00>;

		cpu@0 {
			compatible = "qcom,krait";
			enable-method = "qcom,kpss-acc-v1";
			device_type = "cpu";
			reg = <0x00>;
			next-level-cache = <0x02>;
			qcom,acc = <0x03>;
			qcom,saw = <0x04>;
			cpu-idle-states = <0x05>;
			phandle = <0x6a>;
		};

		cpu@1 {
			compatible = "qcom,krait";
			enable-method = "qcom,kpss-acc-v1";
			device_type = "cpu";
			reg = <0x01>;
			next-level-cache = <0x02>;
			qcom,acc = <0x06>;
			qcom,saw = <0x07>;
			cpu-idle-states = <0x05>;
			phandle = <0x6c>;
		};

		cpu@2 {
			compatible = "qcom,krait";
			enable-method = "qcom,kpss-acc-v1";
			device_type = "cpu";
			reg = <0x02>;
			next-level-cache = <0x02>;
			qcom,acc = <0x08>;
			qcom,saw = <0x09>;
			cpu-idle-states = <0x05>;
			phandle = <0x6e>;
		};

		cpu@3 {
			compatible = "qcom,krait";
			enable-method = "qcom,kpss-acc-v1";
			device_type = "cpu";
			reg = <0x03>;
			next-level-cache = <0x02>;
			qcom,acc = <0x0a>;
			qcom,saw = <0x0b>;
			cpu-idle-states = <0x05>;
			phandle = <0x70>;
		};

		l2-cache {
			compatible = "cache";
			cache-level = <0x02>;
			cache-unified;
			phandle = <0x02>;
		};

		idle-states {

			cpu-spc {
				compatible = "qcom,idle-state-spc", "arm,idle-state";
				entry-latency-us = <0x190>;
				exit-latency-us = <0x384>;
				min-residency-us = <0xbb8>;
				phandle = <0x05>;
			};
		};
	};

	memory@0 {
		device_type = "memory";
		reg = <0x00 0x00>;
	};

	thermal-zones {

		cpu0-thermal {
			polling-delay-passive = <0xfa>;
			polling-delay = <0x3e8>;
			thermal-sensors = <0x0c 0x07>;
			coefficients = <0x4af 0x00>;

			trips {

				trip0 {
					temperature = <0x124f8>;
					hysteresis = <0x7d0>;
					type = "passive";
				};

				trip1 {
					temperature = <0x1adb0>;
					hysteresis = <0x7d0>;
					type = "critical";
				};
			};
		};

		cpu1-thermal {
			polling-delay-passive = <0xfa>;
			polling-delay = <0x3e8>;
			thermal-sensors = <0x0c 0x08>;
			coefficients = <0x46c 0x00>;

			trips {

				trip0 {
					temperature = <0x124f8>;
					hysteresis = <0x7d0>;
					type = "passive";
				};

				trip1 {
					temperature = <0x1adb0>;
					hysteresis = <0x7d0>;
					type = "critical";
				};
			};
		};

		cpu2-thermal {
			polling-delay-passive = <0xfa>;
			polling-delay = <0x3e8>;
			thermal-sensors = <0x0c 0x09>;
			coefficients = <0x4af 0x00>;

			trips {

				trip0 {
					temperature = <0x124f8>;
					hysteresis = <0x7d0>;
					type = "passive";
				};

				trip1 {
					temperature = <0x1adb0>;
					hysteresis = <0x7d0>;
					type = "critical";
				};
			};
		};

		cpu3-thermal {
			polling-delay-passive = <0xfa>;
			polling-delay = <0x3e8>;
			thermal-sensors = <0x0c 0x0a>;
			coefficients = <0x46c 0x00>;

			trips {

				trip0 {
					temperature = <0x124f8>;
					hysteresis = <0x7d0>;
					type = "passive";
				};

				trip1 {
					temperature = <0x1adb0>;
					hysteresis = <0x7d0>;
					type = "critical";
				};
			};
		};
	};

	cpu-pmu {
		compatible = "qcom,krait-pmu";
		interrupts = <0x01 0x0a 0x304>;
	};

	clocks {

		cxo_board {
			compatible = "fixed-clock";
			#clock-cells = <0x00>;
			clock-frequency = <0x124f800>;
			phandle = <0x2d>;
		};

		pxo_board {
			compatible = "fixed-clock";
			#clock-cells = <0x00>;
			clock-frequency = <0x19bfcc0>;
			phandle = <0x16>;
		};

		sleep_clk {
			compatible = "fixed-clock";
			#clock-cells = <0x00>;
			clock-frequency = <0x8000>;
			phandle = <0x38>;
		};
	};

	hwmutex {
		compatible = "qcom,sfpb-mutex";
		syscon = <0x0d 0x604 0x04>;
		#hwlock-cells = <0x01>;
		phandle = <0x0f>;
	};

	smem {
		compatible = "qcom,smem";
		memory-region = <0x0e>;
		hwlocks = <0x0f 0x03>;
	};

	smsm {
		compatible = "qcom,smsm";
		#address-cells = <0x01>;
		#size-cells = <0x00>;
		qcom,ipc-1 = <0x10 0x08 0x04>;
		qcom,ipc-2 = <0x10 0x08 0x0e>;
		qcom,ipc-3 = <0x10 0x08 0x17>;
		qcom,ipc-4 = <0x11 0x4094 0x00>;

		apps@0 {
			reg = <0x00>;
			#qcom,smem-state-cells = <0x01>;
			phandle = <0x5f>;
		};

		modem@1 {
			reg = <0x01>;
			interrupts = <0x00 0x26 0x01>;
			interrupt-controller;
			#interrupt-cells = <0x02>;
		};

		q6@2 {
			reg = <0x02>;
			interrupts = <0x00 0x59 0x01>;
			interrupt-controller;
			#interrupt-cells = <0x02>;
		};

		wcnss@3 {
			reg = <0x03>;
			interrupts = <0x00 0xcc 0x01>;
			interrupt-controller;
			#interrupt-cells = <0x02>;
			phandle = <0x5c>;
		};

		dsps@4 {
			reg = <0x04>;
			interrupts = <0x00 0x89 0x01>;
			interrupt-controller;
			#interrupt-cells = <0x02>;
		};
	};

	firmware {

		scm {
			compatible = "qcom,scm-apq8064", "qcom,scm";
			clocks = <0x12 0x0a>;
			clock-names = "core";
		};
	};

	soc {
		#address-cells = <0x01>;
		#size-cells = <0x01>;
		ranges;
		compatible = "simple-bus";

		pinctrl@800000 {
			compatible = "qcom,apq8064-pinctrl";
			reg = <0x800000 0x4000>;
			gpio-controller;
			gpio-ranges = <0x13 0x00 0x00 0x5a>;
			#gpio-cells = <0x02>;
			interrupt-controller;
			#interrupt-cells = <0x02>;
			interrupts = <0x00 0x10 0x04>;
			pinctrl-names = "default";
			pinctrl-0 = <0x14>;
			phandle = <0x13>;

			sdc4-gpios {
				phandle = <0x46>;

				pios {
					pins = "gpio63", "gpio64", "gpio65", "gpio66", "gpio67", "gpio68";
					function = "sdc4";
				};
			};

			sdcc1-pin-active {
				phandle = <0x4a>;

				clk {
					pins = "sdc1_clk";
					drive-strengh = <0x10>;
					bias-disable;
				};

				cmd {
					pins = "sdc1_cmd";
					drive-strengh = <0x0a>;
					bias-pull-up;
				};

				data {
					pins = "sdc1_data";
					drive-strengh = <0x0a>;
					bias-pull-up;
				};
			};

			sdcc3-pin-active {

				clk {
					pins = "sdc3_clk";
					drive-strengh = <0x08>;
					bias-disable;
				};

				cmd {
					pins = "sdc3_cmd";
					drive-strengh = <0x08>;
					bias-pull-up;
				};

				data {
					pins = "sdc3_data";
					drive-strengh = <0x08>;
					bias-pull-up;
				};
			};

			ps_hold {
				phandle = <0x14>;

				mux {
					pins = "gpio78";
					function = "ps_hold";
				};
			};

			i2c1 {
				phandle = <0x18>;

				mux {
					pins = "gpio20", "gpio21";
					function = "gsbi1";
				};

				pinconf {
					pins = "gpio20", "gpio21";
					drive-strength = <0x10>;
					bias-disable;
				};
			};

			i2c1_pins_sleep {
				phandle = <0x19>;

				mux {
					pins = "gpio20", "gpio21";
					function = "gpio";
				};

				pinconf {
					pins = "gpio20", "gpio21";
					drive-strength = <0x02>;
					bias-disable;
				};
			};

			gsbi1_uart_2pins {

				mux {
					pins = "gpio18", "gpio19";
					function = "gsbi1";
				};
			};

			gsbi1_uart_4pins {

				mux {
					pins = "gpio18", "gpio19", "gpio20", "gpio21";
					function = "gsbi1";
				};
			};

			i2c2 {
				phandle = <0x1a>;

				mux {
					pins = "gpio24", "gpio25";
					function = "gsbi2";
				};

				pinconf {
					pins = "gpio24", "gpio25";
					drive-strength = <0x10>;
					bias-disable;
				};
			};

			i2c2_pins_sleep {
				phandle = <0x1b>;

				mux {
					pins = "gpio24", "gpio25";
					function = "gpio";
				};

				pinconf {
					pins = "gpio24", "gpio25";
					drive-strength = <0x02>;
					bias-disable;
				};
			};

			i2c3 {
				phandle = <0x1c>;

				mux {
					pins = "gpio8", "gpio9";
					function = "gsbi3";
				};

				pinconf {
					pins = "gpio8", "gpio9";
					drive-strength = <0x10>;
					bias-disable;
				};
			};

			i2c3_pins_sleep {
				phandle = <0x1d>;

				mux {
					pins = "gpio8", "gpio9";
					function = "gpio";
				};

				pinconf {
					pins = "gpio8", "gpio9";
					drive-strength = <0x02>;
					bias-disable;
				};
			};

			i2c4 {
				phandle = <0x1f>;

				mux {
					pins = "gpio12", "gpio13";
					function = "gsbi4";
				};

				pinconf {
					pins = "gpio12", "gpio13";
					drive-strength = <0x10>;
					bias-disable;
				};
			};

			i2c4_pins_sleep {
				phandle = <0x20>;

				mux {
					pins = "gpio12", "gpio13";
					function = "gpio";
				};

				pinconf {
					pins = "gpio12", "gpio13";
					drive-strength = <0x02>;
					bias-disable;
				};
			};

			spi5_default {
				phandle = <0x21>;

				pinmux {
					pins = "gpio51", "gpio52", "gpio54";
					function = "gsbi5";
				};

				pinmux_cs {
					function = "gpio";
					pins = "gpio53";
				};

				pinconf {
					pins = "gpio51", "gpio52", "gpio54";
					drive-strength = <0x10>;
					bias-disable;
				};

				pinconf_cs {
					pins = "gpio53";
					drive-strength = <0x10>;
					bias-disable;
					output-high;
				};
			};

			spi5_sleep {
				phandle = <0x22>;

				pinmux {
					function = "gpio";
					pins = "gpio51", "gpio52", "gpio53", "gpio54";
				};

				pinconf {
					pins = "gpio51", "gpio52", "gpio53", "gpio54";
					drive-strength = <0x02>;
					bias-pull-down;
				};
			};

			i2c6 {
				phandle = <0x24>;

				mux {
					pins = "gpio16", "gpio17";
					function = "gsbi6";
				};

				pinconf {
					pins = "gpio16", "gpio17";
					drive-strength = <0x10>;
					bias-disable;
				};
			};

			i2c6_pins_sleep {
				phandle = <0x25>;

				mux {
					pins = "gpio16", "gpio17";
					function = "gpio";
				};

				pinconf {
					pins = "gpio16", "gpio17";
					drive-strength = <0x02>;
					bias-disable;
				};
			};

			gsbi4-uart-pin-active-state {
				phandle = <0x1e>;

				rx-pins {
					pins = "gpio11";
					function = "gsbi4";
					drive-strength = <0x02>;
					bias-disable;
				};

				tx-pins {
					pins = "gpio10";
					function = "gsbi4";
					drive-strength = <0x04>;
					bias-disable;
				};
			};

			gsbi6_uart_2pins {

				mux {
					pins = "gpio14", "gpio15";
					function = "gsbi6";
				};
			};

			gsbi6_uart_4pins {
				phandle = <0x23>;

				mux {
					pins = "gpio14", "gpio15", "gpio16", "gpio17";
					function = "gsbi6";
				};
			};

			gsbi7_uart_2pins {
				phandle = <0x26>;

				mux {
					pins = "gpio82", "gpio83";
					function = "gsbi7";
				};
			};

			gsbi7_uart_4pins {

				mux {
					pins = "gpio82", "gpio83", "gpio84", "gpio85";
					function = "gsbi7";
				};
			};

			i2c7 {
				phandle = <0x27>;

				mux {
					pins = "gpio84", "gpio85";
					function = "gsbi7";
				};

				pinconf {
					pins = "gpio84", "gpio85";
					drive-strength = <0x10>;
					bias-disable;
				};
			};

			i2c7_pins_sleep {
				phandle = <0x28>;

				mux {
					pins = "gpio84", "gpio85";
					function = "gpio";
				};

				pinconf {
					pins = "gpio84", "gpio85";
					drive-strength = <0x02>;
					bias-disable;
				};
			};

			riva-fm-active {
				pins = "gpio14", "gpio15";
				function = "riva_fm";
			};

			riva-bt-active {
				pins = "gpio16", "gpio17";
				function = "riva_bt";
			};

			riva-wlan-active {
				pins = "gpio64", "gpio65", "gpio66", "gpio67", "gpio68";
				function = "riva_wlan";
				drive-strength = <0x06>;
				bias-pull-down;
			};

			hdmi-pinctrl {
				phandle = <0x55>;

				mux {
					pins = "gpio70", "gpio71", "gpio72";
					function = "hdmi";
				};

				pinconf_ddc {
					pins = "gpio70", "gpio71";
					bias-pull-up;
					drive-strength = <0x02>;
				};

				pinconf_hpd {
					pins = "gpio72";
					bias-pull-down;
					drive-strength = <0x10>;
				};
			};

			card_detect {
				phandle = <0x44>;

				mux {
					pins = "gpio26";
					function = "gpio";
					bias-disable;
				};
			};

			pcie_pinmux {
				phandle = <0x54>;

				mux {
					pins = "gpio27";
					function = "gpio";
				};

				conf {
					pins = "gpio27";
					drive-strength = <0x0c>;
					bias-disable;
				};
			};
		};

		syscon@1200000 {
			compatible = "syscon";
			reg = <0x1200000 0x8000>;
			phandle = <0x0d>;
		};

		interrupt-controller@2000000 {
			compatible = "qcom,msm-qgic2";
			interrupt-controller;
			#interrupt-cells = <0x03>;
			reg = <0x2000000 0x1000 0x2002000 0x1000>;
			phandle = <0x01>;
		};

		timer@200a000 {
			compatible = "qcom,kpss-wdt-apq8064", "qcom,kpss-timer", "qcom,msm-timer";
			interrupts = <0x01 0x01 0x301 0x01 0x02 0x301 0x01 0x03 0x301>;
			reg = <0x200a000 0x100>;
			clock-frequency = <0x19bfcc0>;
			cpu-offset = <0x80000>;
		};

		clock-controller@2088000 {
			compatible = "qcom,kpss-acc-v1";
			reg = <0x2088000 0x1000 0x2008000 0x1000>;
			clocks = <0x15 0x11f 0x16>;
			clock-names = "pll8_vote", "pxo";
			clock-output-names = "acpu0_aux";
			#clock-cells = <0x00>;
			phandle = <0x03>;
		};

		clock-controller@2098000 {
			compatible = "qcom,kpss-acc-v1";
			reg = <0x2098000 0x1000 0x2008000 0x1000>;
			clocks = <0x15 0x11f 0x16>;
			clock-names = "pll8_vote", "pxo";
			clock-output-names = "acpu1_aux";
			#clock-cells = <0x00>;
			phandle = <0x06>;
		};

		clock-controller@20a8000 {
			compatible = "qcom,kpss-acc-v1";
			reg = <0x20a8000 0x1000 0x2008000 0x1000>;
			clocks = <0x15 0x11f 0x16>;
			clock-names = "pll8_vote", "pxo";
			clock-output-names = "acpu2_aux";
			#clock-cells = <0x00>;
			phandle = <0x08>;
		};

		clock-controller@20b8000 {
			compatible = "qcom,kpss-acc-v1";
			reg = <0x20b8000 0x1000 0x2008000 0x1000>;
			clocks = <0x15 0x11f 0x16>;
			clock-names = "pll8_vote", "pxo";
			clock-output-names = "acpu3_aux";
			#clock-cells = <0x00>;
			phandle = <0x0a>;
		};

		power-manager@2089000 {
			compatible = "qcom,apq8064-saw2-v1.1-cpu", "qcom,saw2";
			reg = <0x2089000 0x1000 0x2009000 0x1000>;
			phandle = <0x04>;

			regulator {
				regulator-min-microvolt = <0xcf850>;
				regulator-max-microvolt = <0x13d620>;
			};
		};

		power-manager@2099000 {
			compatible = "qcom,apq8064-saw2-v1.1-cpu", "qcom,saw2";
			reg = <0x2099000 0x1000 0x2009000 0x1000>;
			phandle = <0x07>;

			regulator {
				regulator-min-microvolt = <0xcf850>;
				regulator-max-microvolt = <0x13d620>;
			};
		};

		power-manager@20a9000 {
			compatible = "qcom,apq8064-saw2-v1.1-cpu", "qcom,saw2";
			reg = <0x20a9000 0x1000 0x2009000 0x1000>;
			phandle = <0x09>;

			regulator {
				regulator-min-microvolt = <0xcf850>;
				regulator-max-microvolt = <0x13d620>;
			};
		};

		power-manager@20b9000 {
			compatible = "qcom,apq8064-saw2-v1.1-cpu", "qcom,saw2";
			reg = <0x20b9000 0x1000 0x2009000 0x1000>;
			phandle = <0x0b>;

			regulator {
				regulator-min-microvolt = <0xcf850>;
				regulator-max-microvolt = <0x13d620>;
			};
		};

		sps-sic-non-secure@12100000 {
			compatible = "syscon";
			reg = <0x12100000 0x10000>;
			phandle = <0x11>;
		};

		gsbi@12440000 {
			status = "okay";
			compatible = "qcom,gsbi-v1.0.0";
			cell-index = <0x01>;
			reg = <0x12440000 0x100>;
			clocks = <0x15 0x93>;
			clock-names = "iface";
			#address-cells = <0x01>;
			#size-cells = <0x01>;
			ranges;
			syscon-tcsr = <0x17>;
			qcom,mode = <0x02>;

			serial@12450000 {
				compatible = "qcom,msm-uartdm-v1.3", "qcom,msm-uartdm";
				reg = <0x12450000 0x100 0x12400000 0x03>;
				interrupts = <0x00 0xc1 0x04>;
				clocks = <0x15 0xa0 0x15 0x93>;
				clock-names = "core", "iface";
				status = "disabled";
			};

			i2c@12460000 {
				compatible = "qcom,i2c-qup-v1.1.1";
				pinctrl-0 = <0x18>;
				pinctrl-1 = <0x19>;
				pinctrl-names = "default", "sleep";
				reg = <0x12460000 0x1000>;
				interrupts = <0x00 0xc2 0x04>;
				clocks = <0x15 0xb8 0x15 0x93>;
				clock-names = "core", "iface";
				#address-cells = <0x01>;
