						phandle = <0x67>;
					};
				};
			};
		};

		etm@1a1f000 {
			compatible = "arm,coresight-etm3x", "arm,primecell";
			reg = <0x1a1f000 0x1000>;
			clocks = <0x12 0x08>;
			clock-names = "apb_pclk";
			cpu = <0x70>;

			out-ports {

				port {

					endpoint {
						remote-endpoint = <0x71>;
						phandle = <0x68>;
					};
				};
			};
		};
	};

	iio-hwmon {
		compatible = "iio-hwmon";
		io-channels = <0x72 0x00 0x01 0x72 0x00 0x02 0x72 0x00 0x04 0x72 0x00 0x0b 0x72 0x00 0x0c 0x72 0x00 0x0d 0x72 0x00 0x0e>;
	};

	aliases {
		serial0 = "/soc/gsbi@16600000/serial@16640000";
		serial1 = "/soc/gsbi@16500000/serial@16540000";
		i2c0 = "/soc/gsbi@12440000/i2c@12460000";
		i2c1 = "/soc/gsbi@12480000/i2c@124a0000";
		i2c2 = "/soc/gsbi@16200000/i2c@16280000";
		i2c3 = "/soc/gsbi@16300000/i2c@16380000";
		spi0 = "/soc/gsbi@1a200000/spi@1a280000";
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};

	leds {
		compatible = "gpio-leds";
		pinctrl-names = "default";
		pinctrl-0 = <0x73>;

		led-user1 {
			label = "apq8064:green:user1";
			color = <0x02>;
			gpios = <0x2c 0x12 0x00>;
			default-state = "on";
		};
	};

	hdmi-out {
		compatible = "hdmi-connector";
		type = "d";

		port {

			endpoint {
				remote-endpoint = <0x74>;
				phandle = <0x58>;
			};
		};
	};

	pwrseq-sdcc4 {
		pinctrl-names = "default";
		pinctrl-0 = <0x75>;
		compatible = "mmc-pwrseq-simple";
		reset-gpios = <0x2c 0x2b 0x01>;
		phandle = <0x49>;
	};

	regulator-ext-3p3v {
		compatible = "regulator-fixed";
		regulator-min-microvolt = <0x325aa0>;
		regulator-max-microvolt = <0x325aa0>;
		regulator-name = "ext_3p3v";
		startup-delay-us = <0x00>;
		gpio = <0x13 0x4d 0x00>;
		enable-active-high;
		regulator-boot-on;
		phandle = <0x47>;
	};
};
