			interrupts = <0x00 0x70 0x04>;
			clocks = <0x02 0x1d7>;
			clock-names = "secss";
		};

		rng@10830400 {
			compatible = "samsung,exynos5250-prng";
			reg = <0x10830400 0x200>;
