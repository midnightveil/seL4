		timer@101c0000 {
			compatible = "samsung,exynos5420-mct", "samsung,exynos4210-mct";
			reg = <0x101c0000 0xb00>;
			interrupts-extended = <0x11 0x17 0x03 0x11 0x17 0x04 0x11 0x19 0x02 0x11 0x19 0x03 0x01 0x00 0x78 0x04 0x01 0x00 0x79 0x04 0x01 0x00 0x7a 0x04 0x01 0x00 0x7b 0x04 0x01 0x00 0x80 0x04 0x01 0x00 0x81 0x04 0x01 0x00 0x82 0x04 0x01 0x00 0x83 0x04>;
